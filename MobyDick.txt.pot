msgstr ""

msgctxt "45"
msgid "     WHALE,               ICELANDIC."
msgstr ""

msgctxt "46"
msgid "     WHALE,               ENGLISH."
msgstr ""

msgctxt "47"
msgid "     BALEINE,             FRENCH."
msgstr ""

msgctxt "48"
msgid "     BALLENA,             SPANISH."
